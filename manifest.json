{
  "manifest_version": 3,
  "name": "AutoDucker",
  "version": "1.0",
  "permissions": ["tabs", "scripting"],
  "host_permissions": [
    "*://*.youtube.com/*",
    "*://*.spotify.com/*"
  ],
  "background": {
    "service_worker": "src/worker.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*", "*://*.spotify.com/*"],
      "js": ["src/contentScript.js", "src/worker.js"]
    }
  ]
}